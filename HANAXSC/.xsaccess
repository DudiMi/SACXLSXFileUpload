{
    "exposed": true,
    "cors": {
        "enabled": true,
        "allowMethods": [
            "GET",
            "POST",
            "HEAD",
            "OPTIONS"
        ],
        "allowHeaders": [
            "Accept",
            "Authorization",
            "Content-Type",
            "X-CSRF-Token"
        ],
        "allowOrigin": [
            "*"
        ],
        "maxAge": "3600"
    },
    "headers": {
        "enabled": true
    },
    "exposeHeaders": [
        "x-csrf-token"
    ]
}
